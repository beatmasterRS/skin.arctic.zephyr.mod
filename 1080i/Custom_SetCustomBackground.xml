                <onright>9500</onright>
                <orientation>horizontal</orientation>
                <scrolltime>200</scrolltime>
                <itemlayout height="160" width="160">
                    <control type="image">
                        <left>0</left>
                        <top>0</top>
                        <bottom>0</bottom>
                        <right>0</right>
                        <texture>$INFO[ListItem.Thumb]</texture>
                    </control>
                    <control type="image">
                        <left>0</left>
                        <top>0</top>
                        <bottom>0</bottom>
                        <right>0</right>
                        <texture colordiffuse="Highlight">common/white.png</texture>
                        <visible>String.IsEqual(ListItem.Thumb,special://skin/extras/icons/error.png)</visible>
                    </control>
                    <control type="image">
                        <centerright>24</centerright>
                        <centertop>24</centertop>
                        <width>32</width>
                        <height>32</height>
                        <aspectratio aligny="top">keep</aspectratio>
                        <visible>[String.IsEqual(ListItem.Label,Skin.String(backgroundcolor.name)) + !String.IsEqual(ListItem.Label2,selected)]</visible>
                        <visible>!String.IsEqual(ListItem.Thumb,special://skin/extras/icons/error.png) | String.IsEmpty(Skin.String(backgroundcolor.name))</visible>
                        <texture colordiffuse="Black">indicator/checkmark.png</texture>
                    </control>
                    <control type="image">
                        <centerright>24</centerright>
                        <centertop>24</centertop>
                        <width>32</width>
                        <height>32</height>
                        <aspectratio aligny="top">keep</aspectratio>
                        <visible>String.IsEqual(ListItem.Thumb,special://skin/extras/icons/error.png)</visible>
                        <texture colordiffuse="Black">indicator/new.png</texture>
                    </control>
                </itemlayout>

                <left>42</left>
                <width>16</width>
                <height>16</height>
                <texture colordiffuse="$VAR[ColorHighlight]">osd/minicheck.png</texture>
                <visible>Playlist.IsRepeat | Playlist.IsRepeatOne</visible>
                <visible>Control.HasFocus(1)</visible>
            </control>
            <control type="image" description="repeat-check-fo">
                <top>42</top>
                <left>42</left>
                <width>16</width>
                <height>16</height>
                <texture colordiffuse="PanelWhite70">osd/minicheck.png</texture>
                <visible>Playlist.IsRepeat | Playlist.IsRepeatOne</visible>
                <visible>!Control.HasFocus(1)</visible>
            </control>

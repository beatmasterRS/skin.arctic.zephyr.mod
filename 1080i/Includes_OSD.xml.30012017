    <variable name="OSDPanelWhite30">
        <value condition="[Skin.HasSetting(osd.usetheme)] | [!Window.IsVisible(fullscreenvideo) + !Window.IsVisible(visualisation)]">Dark2</value>
        <value>PanelWhite30</value>

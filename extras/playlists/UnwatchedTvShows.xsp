<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="tvshows">
    <name>Unwatched TVShows</name>
    <match>all</match>
    <rule field="numwatched" operator="is">
        <value>0</value>
    </rule>
    <rule field="numepisodes" operator="greaterthan">
        <value>0</value>
    </rule>
    <limit>100</limit>
</smartplaylist>
